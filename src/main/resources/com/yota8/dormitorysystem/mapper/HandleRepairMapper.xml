<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yota8.dormitorysystem.mapper.HandleRepairMapper">

    <update id="finishRepair">
        update repair
        set complete = 1
        where repairId = #{id};
    </update>

</mapper>
